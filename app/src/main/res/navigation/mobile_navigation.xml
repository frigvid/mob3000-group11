<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_docs"
        android:name="no.usn.mob3000.ui.docs.DocsFragment"
        android:label="@string/docs_title"
        tools:layout="@layout/fragment_docs" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="no.usn.mob3000.ui.news.NewsFragment"
        android:label="@string/news_title"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="no.usn.mob3000.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="no.usn.mob3000.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="no.usn.mob3000.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>